# name: application
# key: application
# --
-module(${1:}).

-behaviour(application).

-export([start/2, stop/1]).

start(_Type, _StartArgs) ->
    case ${0:root_supervisor}:start_link() of
        {ok, Pid} ->
            {ok, Pid};
        Other ->
            {error, Other}
    end.

stop(_State) ->
    ok.