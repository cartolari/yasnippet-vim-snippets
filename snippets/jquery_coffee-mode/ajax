# name: ajax
# key: ajax
# --
$.ajax
  url: "${1:mydomain.com/url}"
  type: "${2:POST}"
  dataType: "${3:xml/html/script/json}"
  data: ${4:data}
  complete: (jqXHR, textStatus) ->
	${5:// callback}
  success: (data, textStatus, jqXHR) ->
	${6:// success callback}
  error: (jqXHR, textStatus, errorThrown) ->
	${0:// error callback}